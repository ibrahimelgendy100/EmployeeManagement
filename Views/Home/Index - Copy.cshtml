@model IEnumerable<Employee>

	<table>
		<thead>
			<tr>
				<th>
					ID
				</th>
				<th>
					Name
				</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var emp in Model)
			{
			<tr>
				<td>
					@emp.Id
				</td>
				<td>
					@emp.Name
				</td>
				<td>
					@emp.Department
				</td>
			</tr>
			}
		</tbody>
	</table>
